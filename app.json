{
  "expo": {
    "name": "Lok Lagbe",
    "slug": "LokLagbe",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "android": {
      "package": "com.loklagbe.loklagbe"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#fafafa"
    }
  },
  "packagerOpts": {
    "config": "metro.config.js"
  }
}